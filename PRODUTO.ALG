Algoritmo "produto"

Var
numero: real

Inicio
escreva(" digite um numero: ")
  leia(numero)

  se numero> 0 entao
   escreva("o numero e positivo")
  senao
   se numero < -0 entao
      escreva("o numero e negativo.")
    senao
     se numero = 0 entao
     escreva("O numero e nulo.")
     fimse
      fimse
       fimse
     




Fimalgoritmo